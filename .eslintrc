{
    "extends": "xo/esnext",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["error", {
            "code": 120,
            "ignoreUrls": true
        }],
        "babel/object-curly-spacing": ["error", "always"],
        "object-curly-spacing": ["error", "always"],
        "no-console": ["error"]
    }
}
